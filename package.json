{
  "name": "Astronomy",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:build": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --assets-dest 'ios' --dev=false --platform='ios'",
    "android:build": "cd android && ./gradlew assembleRelease",
    "start": "react-native start",
    "gen": "graphql-codegen --config codegen.yml",
    "test": "jest",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "ts:check": "tsc --watch",
    "ts:check:ci": "tsc"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@react-native-community/art": "^1.2.0",
    "@react-native-community/async-storage": "^1.12.0",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/datetimepicker": "^2.6.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.5",
    "@react-native-community/picker": "^1.6.5",
    "@react-navigation/bottom-tabs": "^5.7.1",
    "@react-navigation/drawer": "^5.8.5",
    "@react-navigation/native": "^5.7.0",
    "@react-navigation/stack": "^5.7.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "dayjs": "^1.8.30",
    "formik": "^2.1.5",
    "jwt-decode": "^2.2.0",
    "node-fetch": "^2.6.1",
    "react": "16.13.1",
    "react-native": "0.64.1",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-localize": "^1.4.1",
    "react-native-modal": "^11.5.6",
    "react-native-progress": "^4.1.2",
    "react-native-reanimated": "^1.9.0",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-screens": "^2.9.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-ui-lib": "^5.11.0",
    "react-native-vector-icons": "^7.0.0",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@graphql-codegen/cli": "^1.17.0",
    "@graphql-codegen/typescript": "^1.17.0",
    "@graphql-codegen/typescript-operations": "^1.17.0",
    "@graphql-codegen/typescript-react-apollo": "^1.17.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.14",
    "@types/jwt-decode": "^2.2.1",
    "@types/react": "^16.9.43",
    "@types/react-native": "^0.63.1",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/yup": "^0.29.3",
    "babel-jest": "^25.1.0",
    "babel-plugin-import-graphql": "^2.7.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.5.1",
    "eslint-plugin-i18n": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-react-native": "^3.10.0",
    "graphql": "^15.3.0",
    "jest": "^25.1.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^10.4.0",
    "metro-react-native-babel-preset": "^0.63.0",
    "prettier": "^2.0.5",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.9.7"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/testsSetup/communityMock.ts",
      "<rootDir>/testsSetup/localizeMock.ts",
      "<rootDir>/testsSetup/modulesMock.ts",
      "<rootDir>/testsSetup/navigationMock.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub"
    }
  }
}
